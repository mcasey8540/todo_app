<% @lists.each do |list| %>
	<% unless list.nil? %>
<table class= "table table-striped table-bordered"
	<tr>
		<th>My Lists</th>
		<th>Options</th>
	</tr>
	<tr>
		<td width="200"><%= link_to "#{list.name}", list_path(list) %></td>
		<td width="100"><%= link_to "Edit", edit_list_path(list), class: "btn btn-warning" %> <%= link_to "Delete", list_path(list), :confirm => "Sure you want to delete list?", :method => :delete, class: "btn btn-danger"%></td>
	</tr>
	<% end %>	
<% end %>
</table> 

<%= link_to "Add New List", new_list_path, class: "btn btn-info" %>
